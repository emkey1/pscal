program EnumForLoop;

type
  Day = (Mon, Tue, Wed, Thu, Fri, Sat, Sun);
  DaySet = set of Day;
  Person = record
    name: string;
    busy: DaySet;
  end;

var
  p: Person;
  d: Day;
begin
  p.name := 'Alice';
  p.busy := [Mon, Wed];
  for d := Mon to Sun do
    if d in p.busy then
      writeln(p.name, ' is busy on ', d);
end.
