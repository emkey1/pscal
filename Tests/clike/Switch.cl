int main() {
    int x = 2;
    switch (x) {
        case 1: printf("A\n"); break;
        case 2: printf("B\n"); break;
        default: printf("D\n");
    }

    x = 3;
    switch (x) {
        case 1: printf("A\n"); break;
        case 2: printf("B\n"); break;
        default: printf("D\n");
    }
    return 0;
}

