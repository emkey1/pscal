#!/bin/exsh
# Tiny compiler wrapper for iOS: run the clike source via shell builtin.
# Prefer sandbox root; fall back to relative bin path.
if [ -n "$PSCALI_WORKSPACE_ROOT" ]; then
  SCRIPT="$PSCALI_WORKSPACE_ROOT/bin/tiny.clike"
else
  SCRIPT="$(dirname "$(dirname "$0")")/bin/tiny.clike"
fi
clike "$SCRIPT" "$@"
